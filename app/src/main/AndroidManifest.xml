        <activity
            android:name=".TraineeEditProfile"
            android:exported="false" />
        <activity
            android:name=".TraineeProfile"
            android:exported="false" />
        <activity
            android:name=".TraineeDashboard"
            android:exported="false" />
        <activity
            android:name=".TrainerProfile"
            android:exported="false" />
        <activity
            android:name=".UserList"
            android:exported="false" />
        <activity
            android:name=".ChatActivity"
            android:exported="false" />
        <activity
            android:name=".WaitingJoinActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.example.fmgymtrainer.intent.action.JoinBeforeHost" />
                <action android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ZoomApp"
            android:exported="false" />
        <activity
            android:name=".TrainerProfileVC"
            android:exported="false" />
        <activity
            android:name=".TrainerList"
            android:exported="false" />
        <activity
            android:name=".ViewProfile"
